{
  "name": "flc-pokemon-gen",
  "version": "0.1.0",
  "description": "A module for Pokemon creation utility methods for FLC v2.0.",
  "main": "dist/index",
  "types": "dist/index",
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "scripts": {
    "prebuild": "rm -rf ./dist && mkdir dist",
    "build": "tsc",
    "publish": "npm run build && cp ./package.json ./dist/package.json",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3"
  },
  "dependencies": {
    "npm": "^6.0.0"
  }
}
